{
    "openapi": "3.0.0",
    "info": {
        "title": "Image Server Rest",
        "contact": {
            "email": "kyle@waryway.com"
        },
        "version": "1.0.0"
    },
    "paths": {
        "/images": {
            "get": {
                "operationId": "Waryway\\ImageServerRest\\ImageHandler::getImageStatistics",
                "responses": {
                    "200": {
                        "description": "Returns available image statistics",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/ImageMetaData"
                                }
                            }
                        }
                    },
                    "404": {
                        "description": "Returns a 404 if the server cannot find the path to the images.",
                        "content": {
                            "text/html": {}
                        }
                    }
                }
            }
        },
        "/helloWorld": {
            "get": {
                "operationId": "Waryway\\ImageServerRest\\Router::helloWorld",
                "responses": {
                    "200": {
                        "description": "Returns Hello World"
                    }
                }
            }
        },
        "/swagger.json": {
            "get": {
                "operationId": "Waryway\\ImageServerRest\\Router::swagger",
                "responses": {
                    "200": {
                        "description": "The API's swagger.json will be served."
                    },
                    "404": {
                        "description": "If the swagger.json is not present."
                    }
                }
            }
        }
    },
    "components": {
        "schemas": {
            "ImageMetaData": {
                "properties": {
                    "name": {
                        "description": "The Image file name",
                        "type": "string"
                    },
                    "size": {
                        "description": "The size of the image",
                        "type": "string"
                    },
                    "type": {
                        "description": "The type of image",
                        "type": "string"
                    },
                    "url": {
                        "description": "The url to retrieve the image.",
                        "type": "string"
                    }
                },
                "type": "object"
            }
        }
    }
}